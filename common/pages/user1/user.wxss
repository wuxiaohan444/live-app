    right: 20rpx;
    top: -15rpx;
    font-size: 23rpx;
    padding: 0 6rpx;
    border: 1rpx solid #ee5a52;
}

.order-detail .contact-btn {
    line-height: normal;
    background-color: #fff;
    font-size: 12px;
    font-weight: normal;
    color: #646566;
    display: block;
}

.order-detail .icon-image {
    width: 50rpx;
    height: 50rpx;
}

.order-detail .contact-btn:after {
    border: 0;
}

.page-container .nav {
    background-color: #fff;
    border-radius: 6rpx;
    height: 140rpx;
    margin: 0 12px 12px;
}

.page-container .nav .item {
