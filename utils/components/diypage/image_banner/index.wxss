.swiper {
    position: relative;
}

.swiper>swiper {
    height: 420rpx;
}

.swiper-item .img {
}

.swiper-item .img image {
    width: 100%;
    height: 420rpx;
}

.dots {
    display: flex;
    justify-content: center;
    margin-top: 20rpx;
    position: absolute;
    bottom: 20rpx;
    width: 100%;
    text-align: center;
    padding: 0 30rpx;
}

.alignleft {
    justify-content: flex-start;
}

.aligncenter {
    justify-content: center;
}

.alignright {
    justify-content: flex-end;
}

.round {
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    margin: 0 8rpx;
    background-color: #b1a6a3;
}

.round.active {
    width: 20rpx;
    background: #fff;
}

.arc {
    width: 30rpx;
    height: 10rpx;
    border-radius: 12rpx;
    margin: 0 8rpx;
    background-color: #b1a6a3;
}

.arc.active {
    width: 30rpx;
    background: #fff;
}

.square {
    width: 30rpx;
    height: 10rpx;
    margin: 0 8rpx;
    background-color: #b1a6a3;
}

.square.active {
    width: 30rpx;
    background: #fff;
}