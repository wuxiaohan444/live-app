.commission-header {
    width: 95%;
    height: 150rpx;
    background: #ff7664;
    margin: 20rpx;
    border-radius: 5px;
    padding-left: 30rpx;
    padding-top: 20rpx;
}

.commission-text {
    color: #fff;
    font-size: 12px;
}

.commission-total {
    color: #fff;
    font-weight: bold;
    font-size: 24px;
}

.commission-info-box {
    display: flex;
    flex-direction: row;
    width: 95%;
    height: 120rpx;
    margin-top: 0rpx;
    margin-left: 20rpx;
    background: #fff;
    border-bottom: #eee solid 1px;
}

.commission-info-box .commission-info-detail-box {
    display: flex;
    flex-direction: column;
    width: 675rpx;
    margin-top: 0rpx;
}

.commission-info-detail-box .commission-name-text {
    height: 50rpx;
    padding-left: 21rpx;
    padding-top: 12rpx;
    font-size: 32rpx;
    font-weight: 800;
    font-family: PingFangSC-Light;
    color: rgba(0,0,0,1);
}

.commission-info-detail-box .commission-time-box {
    display: flex;
    flex-direction: row;
    height: 53rpx;
    padding-left: 21rpx;
    padding-top: 12rpx;
}

.commission-info-box .commission-price {
    width: 38rpx;
    line-height: 60rpx;
    margin-right: 35rpx;
    margin-top: 26rpx;
    padding-right: 50rpx;
    font-size: 32rpx;
    font-weight: 400;
}

.empty-content {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
}