.commission-header {
    height: 150rpx;
    background: #ff7664;
    border-radius: 5px;
    padding-left: 30rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    margin: 30rpx;
}

.commission-text {
    color: #fff;
    font-size: 12px;
    margin-top: 15rpx;
}

.commission-total {
    color: #fff;
    font-weight: bold;
    font-size: 24px;
}

.content {
}

.content>.list-box {
    border-bottom: solid 1rpx #f4f4f4;
    padding: 0 30rpx 30rpx 30rpx;
    background: #fff;
    margin-top: 30rpx;
}

.content>.list-box>.time {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 24rpx;
    height: 90rpx;
    border-bottom: solid 1rpx #f4f4f4;
}

.content>.list-box>.list {
    display: flex;
    padding-top: 30rpx;
}

.content>.list-box>.list>.img {
    width: 140rpx;
    height: 140rpx;
    flex: 0 0 140rpx;
}

.content>.list-box>.list>.img>image {
    width: 100%;
    height: 100%;
    border-radius: 6rpx;
}

.content>.list-box>.list>.text-box {
    flex: 1;
    padding-left: 30rpx;
}

.content>.list-box>.list>.text-box>.name {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    font-weight: 600;
    line-height: 1.5;
    word-wrap: break-word;
    font-size: 28rpx;
}

.content>.list-box>.list>.text-box>.money {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 28rpx;
}

.content>.list-box>.list>.text-box>.money>.m {
    font-weight: 700;
    font-size: 36rpx;
    color: #ff4466;
}

.content>.list-box>.user_address {
    font-size: 26rpx;
    color: #999;
    padding: 10rpx 0;
}

.content>.list-box>.info {
    text-align: right;
    font-size: 26rpx;
    padding: 20rpx 0;
}

.content>.list-box>.lirun {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.content>.list-box>.lirun>.num {
    font-size: 26rpx;
}

.content>.list-box>.lirun>.num>text {
    font-size: 32rpx;
    color: #ff4466;
    font-weight: 700;
}

.content>.list-box>.lirun>.btn {
    background: #ff4466;
    color: #fff;
    display: inline;
    padding: 10rpx 30rpx;
    border-radius: 6rpx;
    font-size: 26rpx;
}