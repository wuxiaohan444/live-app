.school-service {
    background: #fff;
    margin-left: 30rpx;
    margin-right: 30rpx;
    border-radius: 12rpx;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
}

.school-service>.title {
    display: flex;
    align-items: center;
    height: 70rpx;
    padding: 0 30rpx;
    border-bottom: solid 1rpx #f1f1f1;
}

.school-service>.title>.text {
    font-size: 28rpx;
    font-weight: 700;
}

.school-service>.title>.more {
    margin-left: auto;
    font-size: 26rpx;
    color: #666666;
}

.school-service>.title>.img {
    width: 12rpx;
    height: 20rpx;
    margin-left: 8rpx;
}

.school-service>.content {
    padding: 0 30rpx;
}

.school-service>.content>.big-list {
    padding: 30rpx 0;
    border-bottom: solid 1rpx #f1f1f1;
}

.school-service>.content>.big-list>.img {
    width: 100%;
    height: 334rpx;
    border-radius: 12rpx;
}

.school-service>.content>.big-list>.title {
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    white-space: normal!important;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    font-size: 26rpx;
    color: #333;
    font-weight: 600;
    padding-top: 20rpx;
}

.school-service>.content>.big-list>.renshu {
    display: flex;
    align-items: center;
    flex-wrap: wrap;
}

.school-service>.content>.big-list>.renshu>.item {
    flex: 0 0 25%;
    font-size: 22rpx;
    color: #666;
    padding-top: 10rpx;
}

.school-service>.content>.item-list {
    display: flex;
    align-items: unset;
    padding: 30rpx 0;
    border-bottom: solid 1rpx #f1f1f1;
}

.school-service>.content>.item-list:last-child {
    border-bottom: none;
}

.school-service>.content>.item-list>.text {
    flex: 1;
    padding-right: 20rpx;
}

.school-service>.content>.item-list>.text>.title {
    font-size: 26rpx;
    font-weight: 600;
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    white-space: normal!important;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    height: 80rpx;
}

.school-service>.content>.item-list>.text-img>.img {
    width: 240rpx;
    height: 140rpx;
    border-radius: 12rpx;
}

.school-service>.content>.item-list>.text>.form {
    display: flex;
    align-items: center;
    font-size: 22rpx;
    color: #666;
    height: 40rpx;
    margin-top: 20rpx;
}

.school-service>.content>.item-list>.text>.form>.look {
    margin-left: auto;
}

.school-service>.content>.item-list>.text>.form>.look>image {
    width: 27rpx;
    height: 19rpx;
    display: inline-block;
    vertical-align: middle;
}

.school-service>.content>.item-list>.text>.form>.msg {
    margin-left: 30rpx;
}

.school-service>.content>.item-list>.text>.form>.msg>image {
    width: 25rpx;
    height: 18rpx;
    display: inline-block;
    vertical-align: middle;
}

.block-nav-icon {
    margin-left: auto;
}

.ad-box {
    margin: 30rpx;
    height: 230rpx;
    border-radius: 12rpx;
    overflow: hidden;
}

.ad-box .ad-block swiper-item {
}

.ad-box image {
    width: 100%;
    height: 100%;
    border-radius: 0;
}

.tool-tip {
    background: #fc3f3f;
    color: #fff;
    border-radius: 40%;
    position: absolute;
    right: 10rpx;
    top: -20rpx;
    font-size: 23rpx;
    padding: 0 6rpx;
    border: none;
    height: 25rpx;
    line-height: 25rpx;
    padding: 10rpx;
}

.block-nav-icon .more {
    display: inline;
}