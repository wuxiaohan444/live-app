page {
    background: #f4f4f4;
    font-size: 24rpx!important;
}

image {
    display: block;
}

.head {
    background: #fe4564;
    height: 160rpx;
    border-bottom-left-radius: 50rpx;
    border-bottom-right-radius: 50rpx;
}

.user-info {
    display: flex;
    align-items: center;
    padding: 30rpx;
    margin: 0 30rpx;
    background: #fff;
    border-radius: 12rpx;
    margin-top: -100rpx;
    position: relative;
}

.user-info>.img {
    width: 130rpx;
    height: 130rpx;
    border-radius: 100%;
}

.user-info>.user {
    flex: 1;
    color: #333;
    padding: 0 20rpx;
    line-height: 1;
}

.user-info>.user>.name {
    font-size: 30rpx;
    font-weight: 700;
    display: flex;
    align-items: center;
    margin-bottom: 10rpx;
}

.user-info>.user>.name>.img {
    width: 24rpx;
    height: 28rpx;
}

.user-info>.user>.tip {
    font-size: 22rpx;
    font-weight: 500;
    line-height: 1;
    padding: 8rpx 0;
}

.user-info>.user>.tip>text {
    margin-left: 10rpx;
}

.user-info>.user>.goggao {
    color: #999;
    font-size: 20rpx;
    margin-top: 10rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.user-info>.user>.goggao>text {
    background: #fe4564;
    color: #fff;
    padding: 2rpx 10rpx;
    border-radius: 6rpx;
    font-size: 20rpx;
    margin-right: 10rpx;
}

.user-info>.msg {
    position: absolute;
    top: 26rpx;
    right: -10rpx;
    width: 200rpx;
    height: 90rpx;
    background: linear-gradient(141deg,#d9b998 0%,#b68b6b 75%);
    border-top-left-radius: 40rpx;
    border-bottom-left-radius: 40rpx;
    color: #fff;
    font-size: 20rpx;
    z-index: 9;
}

.user-info>.sanjiao {
    background: #b68b6b;
    position: absolute;
    right: -10rpx;
    top: 116rpx;
    width: 10rpx;
    height: 10rpx;
    border-bottom-right-radius: 100%;
}

.user-info>.msg {
    display: flex;
    align-items: center;
    justify-content: center;
}

.user-info>.msg>.box>.text {
    font-weight: 600;
    line-height: 1;
}

.user-info>.msg>.box>.btn {
    display: inline-block;
    background: #fff;
    color: #b68b6b;
    border-radius: 6rpx;
    margin-top: 10rpx;
    padding: 8rpx 10rpx;
    line-height: 1;
}

.certification>.title {
    display: flex;
    align-items: center;
    height: 70rpx;
    margin: 30rpx;
    background: #fff;
    border-bottom: solid 1rpx #f1f1f1;
    padding: 0 30rpx;
    border-radius: 12rpx;
}

.certification>.title>.text {
    font-size: 26rpx;
    color: #666666;
}

.certification>.title>.more {
    margin-left: auto;
    font-size: 26rpx;
    color: #666666;
}

.certification>.title>.img {
    width: 12rpx;
    height: 20rpx;
    margin-left: 8rpx;
}

.certification>.title>.ling {
    width: 38rpx;
    height: 38rpx;
    border-radius: 100%;
    margin-right: 10rpx;
}

